<script>
 /* this is used both by Vega and DataTable for <%= report[:title] %>*/
 data_<%= index %> = [
   <% report[:rows].each do |row| -%>
   {
     <% report[:header].each_with_index do |h, i| -%>
     "<%= h %>": "<%= Emitter::process row[i] %>",
     <% end -%>
   },
   <% end -%>
 ]
</script>
